<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yz.crm.service.mapper.UserMapper">
    <select id="selectUser" resultType="com.yz.crm.common.entity.system.SysUser">
        SELECT
          id,
          name,
          deptId,
          type
        FROM sys_user
    </select>
</mapper>
